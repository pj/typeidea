  },
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@graphql-codegen/fragment-matcher": "^1.7.0",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.4.1",
    "@types/ejs": "^2.6.1",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.12.15",
    "@types/prettier": "^1.15.2",
    "@types/proper-lockfile": "^4.1.1",
    "@types/react": "^16.9.2",
    "@types/supertest": "^2.0.7",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.0",
    "apollo-boost": "^0.4.4",
    "apollo-server": "^2.9.3",
    "classnames": "^2.2.6",
    "ejs": "^2.6.1",
    "enzyme": "^3.9.0",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "graphql-import": "^0.7.1",
    "json-object-mapper": "^1.6.0",
    "lodash": "^4.17.13",
    "parcel": "^1.12.3",
    "prettier": "^1.15.3",
    "proper-lockfile": "^4.1.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "superagent": "^5.0.6",
    "tmp": "^0.1.0",
    "typescript": "^3.2.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/introspection": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@types/jest": "^23.3.10",
    "chokidar-cli": "^1.2.2",
    "jest": "^23.6.0",
    "supertest": "^4.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.42"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "chokidar \"lib/**/*.ejs\" \"lib/**/*.ts\" \"tests/**/*.ts\" \"tests/**/*.json\" -c \"yarn run test\" --ignore runtest --ignore tests/snapshots",
    "build": "tsc --outDir dist --rootDir lib && cp lib/server/schema.graphql dist/server",
    "build:watch": "chokidar \"lib/**/*.ejs\" \"lib/**/*.ts\" \"tests/**/*.ts\" \"server/**/*.ts\" -c \"graphql-codegen --config codegen.yml && tsc --outDir dist --rootDir lib && cp lib/server/schema.graphql dist/server\" --ignore runtest --ignore tests/snapshots",
    "start:client:dev": "parcel -d build_temp lib/client/index.html",
    "start:server:dev": "ts-node-dev lib/server/index.ts",
    "server:generate_types": "graphql-codegen --config codegen.yml"
  }
}
